@model List<Category> 

<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead>
	<tr>
		<th>Category Name</th>
		<th>Display Name</th>
	</tr>
	</thead>
	<tbody>
		@foreach(Category obj in Model.OrderBy(x=>x.DisplayOrder))
		{
			<tr>
				<th>@obj.Name</th>
				<th>@obj.DisplayOrder</th>
			</tr>
		}
	</tbody>
</table>